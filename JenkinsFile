pipeline
{
	agent any
	stages
	{
		stage('Build')
		{
			steps
			{
				sh mvn clean
			}
		}
		stage('Deploy')
		{
			steps
			{
				echo "Deployment of Code is Started"
				echo "Deployment of Code is Successful"
			}
		}
		stage('Tests')
		{
			steps
			{
				sh mvn -PSmoke					
			}
			steps
			{
				sh mvn -PRegression
			}
		}
                stage('Release')
		{
			steps
			{
				echo "Release is Started"
				echo "Release is Successful"
			}
		}
	}
}

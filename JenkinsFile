pipeline
{
	agent any
	stages
	{
		stage('Build')
		{
			steps
			{
				bat "mvn clean"
			}
		}
		stage('Deploy')
		{
			steps
			{
				echo "Deployment of Code is Started"
				echo "Deployment of Code is Successful"
			}
		}
		stage('Test')
		{
			steps
			{
				bat "mvn test -PSmoke"					
			}
		}
                stage('Release')
		{
			steps
			{
				echo "Release is Started"
				echo "Release is Successful"
			}
		}
	}
}
